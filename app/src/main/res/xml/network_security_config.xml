<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
    <debug-overrides>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
            <!-- Tambahkan sertifikat self-signed di bawah ini jika diperlukan -->
            <!-- <certificates src="app" /> -->
        </trust-anchors>
    </debug-overrides>
</network-security-config>